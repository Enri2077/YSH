{
	"manifest_version": 2,

	"name": "Simple Homepage for Youtube™",
	"description": "An alternative homepage for Youtube™",
	"version": "0.1.3",
	"key": "",
	
	"background": {
    	"scripts": ["js/background.js"]
	},
	
	"browser_action": {
		"default_title": "open Youtube Simple Homepage in a new tab",
		"default_icon": {
			"19": "assets/icon_19x19.png",
			"38": "assets/icon_128x128.png"
		}
	},
	
	"options_page": "options/options.html",
	
	"oauth2": {
		"client_id": "921945633910-ek11tf4ng2g01vmjoifdfvvn6cqjulpa.apps.googleusercontent.com",
		"scopes": [
			"https://www.googleapis.com/auth/youtube"
		]
	},
	
	"icons": {
		"16": "assets/icon_16x16.png",
		"128": "assets/icon_128x128.png"
	},

	"permissions": [
		"identity",
		"storage",
		"unlimitedStorage",
		"tabs",
    	"https://ssl.gstatic.com/",
    	"https://www.googleapis.com/",
    	"https://accounts.google.com/"
	]
}
